{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AccessToDynamoDB",
        "Effect": "Allow",
        "Action": [
            "dynamodb:GetItem",
            "dynamodb:PutItem",
            "dynamodb:Scan"
        ],
        "Resource": "${visitors_count}"
      }
    ]
  }
  